        "illuminate/collections": "^11.4",
        "uttamrabadiya/console-components": "^1.0.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.3",
        "phpunit/phpunit": "~5.5|^9.0",
        "phpstan/phpstan": "^1.4",
        "squizlabs/php_codesniffer": "^3.9",
        "friendsofphp/php-cs-fixer": "^3.3",
        "symfony/var-dumper": "^7.0",
        "ext-zip": "*"
    },
    "scripts": {
        "post-install-cmd": [
        ],
        "post-update-cmd": [
        ]
    },
    "bin": [
        "valet"
    ]
